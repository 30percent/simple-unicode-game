{
  "name": "immutable-gametry",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev":
      "webpack-dashboard -- webpack-dev-server --config ./webpack.config.js --hot --inline --progress --watch --open",
    "server:prod":
      "cross-env NODE_ENV=production webpack-dashboard -- webpack-dev-server --config ./webpack.config.js --port 3000 --host 0.0.0.0 --hot --inline --progress --profile --watch --open --content-base dist/",
    "build": "npm run build:prod",
    "build:dev":
      "webpack --config ./webpack.config.js --progress --profile --color --display-error-details --display-cached --mode=development",
    "build:prod":
      "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --profile --color --display-error-details --display-cached --bail",
    "clean": "npm cache clear && rimraf -- dist",
    "test": "karma start",
    "prettier": "pretty-quick --staged"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/lodash": "^4.14.107",
    "awesome-typescript-loader": "^5.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "ejs-loader": "^0.3.0",
    "eslint": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "3.1.3"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/node": "^9.6.6",
    "bluebird": "^3.5.1",
    "cuid": "^2.1.1",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "pre-commit": "^1.2.2",
    "pretty-quick": "^1.4.1"
  },
  "pre-commit": ["prettier"]
}
